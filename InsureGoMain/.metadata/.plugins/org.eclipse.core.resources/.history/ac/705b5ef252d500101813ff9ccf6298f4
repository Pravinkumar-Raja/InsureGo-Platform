package com.example.demo.payload.response;

public class JwtResponse {
private String token;
private String type="Bearer";
private String email;
private String role;

	// ... fields ...
	    public JwtResponse(String token, String type, String email) { // WRONG parameter names/count
	        super();
	        this.token = token;
	        this.type = type; // This is receiving the email or role
	        this.email = email; // This is receiving the role
	        this.role = role; // This is null because the parameter wasn't passed in
	    }

public String getToken() {
	return token;
}
public void setToken(String token) {
	this.token = token;
}
public String getType() {
	return type;
}
public void setType(String type) {
	this.type = type;
}
public String getEmail() {
	return email;
}
public void setEmail(String email) {
	this.email = email;
}
public String getRole() {
	return role;
}
public void setRole(String role) {
	this.role = role;
}



}
