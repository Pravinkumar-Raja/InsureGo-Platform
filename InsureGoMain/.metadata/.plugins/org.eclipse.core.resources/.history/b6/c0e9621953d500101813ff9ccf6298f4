package com.example.demo.payload.response;

public class JwtResponse {
private String token;
private String type="Bearer";
private String email;
private String role;

	// ... fields ...
//Current (INCORRECT) constructor:
public JwtResponse(String token, String type, String email) { // Note the parameter names!
 super();
 this.token = token;
 this.type = type; // The second parameter is being assigned to 'this.type'
 this.email = email; // The third parameter is being assigned to 'this.email'
 //this.role = role; // This assignment uses the field 'role', which hasn't been set by a parameter.
}

public String getToken() {
	return token;
}
public void setToken(String token) {
	this.token = token;
}
public String getType() {
	return type;
}
public void setType(String type) {
	this.type = type;
}
public String getEmail() {
	return email;
}
public void setEmail(String email) {
	this.email = email;
}
public String getRole() {
	return role;
}
public void setRole(String role) {
	this.role = role;
}



}
